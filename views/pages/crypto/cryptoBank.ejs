<%- include('../../partials/head.ejs'); %>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
<body>
    <%- include('../../partials/header.ejs'); %>

    <section class="bankForm">


        <h3>Here Are Users and the Coins They Have in the Bank</h3>
       
        <% crypto.forEach( (crypto, i) => { %>
            <li class="userName">
              <button class="report"><a href="#details<%= i %>"> <%= crypto.user_name %> </a></button>
            </li>
            <div class="details" id="details<%= i %>">
                <br>
                <li class="one">Time Searched: <%= crypto.time_date_stamp %></li>
                <br>
                <li class="symbolName">
                    Coin: <%= crypto.symbol_name1 %>
                </li>
                <li class="price"> 
                    Price: <%= crypto.usd_price1 %> 
                </li>
                <br>
                <li class="symbolName">
                    Coin: <%= crypto.symbol_name2 %>
                </li>
                <li class="price"> 
                    Price: <%= crypto.usd_price2 %> 
                </li>
                <br>
                <li class="symbolName">
                    Coin: <%= crypto.symbol_name3 %>
                </li>
                <li class="price"> 
                    Price: <%= crypto.usd_price3 %> 
                </li>
                <br>
                <li class="symbolName">
                    Coin: <%= crypto.symbol_name4 %>
                </li>
                <li class="price"> 
                    Price: <%= crypto.usd_price4 %> 
                </li>
                <br>
                <li class="symbolName">
                    Coin: <%= crypto.symbol_name5 %>
                </li>
                <li class="price"> 
                    Price: <%= crypto.usd_price5 %> 
                </li>
                <br>
            </div>
        <% }) %>
                
        </section>
        <script src="js/app.js"></script>
</body>
